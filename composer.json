{
  "name": "alpdesk/contao-google-fonts",
  "description": "Local Google Webfonts for Contao",
  "keywords": [
    "contao",
    "Google fonts",
    "Google Webfonts",
    "local Google fonts",
    "local Google Webfonts"
  ],
  "type": "contao-bundle",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "ext-zip": "*",
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.13 || ^5.0",
    "symfony/http-client": "^5.4 || ^6.0",
    "sabberworm/php-css-parser": "8.4.0"
  },
  "conflict": {
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "authors": [
    {
      "name": "Alpdesk",
      "homepage": "https://alpdesk.de",
      "email": "infos@x-projects.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Alpdesk\\AlpdeskGoogleFonts\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Alpdesk\\AlpdeskGoogleFonts\\ContaoManager\\Plugin",
    "contao-component-dir": "public"
  },
  "config": {
    "contao-component-dir": "public",
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "contao-community-alliance/composer-plugin": true,
      "contao-components/installer": true,
      "contao/manager-plugin": true
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "contao/manager-bundle": "^4.13 || ^5.0 ",
    "contao/manager-plugin": "^2.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "slam/phpstan-extensions": "^6.0",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.4 | ^6.0"
  }
}
